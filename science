        _dcq.push(["identify", {
            email: email,
            remove_tags: [tag]
        }]);
    }

    $(document).on("submitted.drip", function(ev, data){
        var csrf_token = 'YeYhXg8smLrSLQFZ6yaG9GoOj1V6Nn6iOExyu2wKLV8bBNRSTJYWXC2tLwax3xlC';
        var email = data["fields[email]"];
        $.ajax({
            url: "/email-subscribe-popup/",
            type: "POST",
