        ga('set', 'exp', experiment); 
        //Need to send event to get optimize experiment set
        ga('send', {
          hitType: 'event',
          eventCategory: EXPERIMENT_VIEWED_EVENT,
          eventAction: k,
          eventLabel: experiment,
        });
      }
    }
  }
  
  window.updateOptimizeAbTests = updateOptimizeAbTests;

