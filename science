  const EXPERIMENT_VIEWED_EVENT = 'experiment-viewed';
  
  var updateSegmentAbTests = function(analytics, experiments){
    for (var k in experiments){
      var val = experiments[k];
      analytics.track(EXPERIMENT_VIEWED_EVENT, 
        {
          'experimentId': k,
          'experimentName': val.experiment_name,
          'variationId': val.variation_id,
          'variationName': val.variation_name,
        }
      );
    }
  }

  window.updateSegmentAbTests = updateSegmentAbTests;

