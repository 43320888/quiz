                var dataProperties = {'email': data.email, 'sourceType': 'dripPopup', 'label': eventName};
                analytics.track('newsletter-signup-button', dataProperties);
                analytics.identify(identifyProperties);
                console.log("success");
            },
            error: function () {
                console.log("error");
            }
        });
    })
